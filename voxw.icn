procedure alternate_mainxyz()
    
$define Palette "c5"			# default palette

#$define DH 30.0 		# change in hue per pixel
#$define DL 10.0 		# change in lightness per pixel

   p := Palette

   every i := 1 to 10 by 1 do {
   every o := 1 to 10 by 1 do { 
   every q := 1 to 10 by 1 do {

      h := integer(i * DH) || ":"
      
         l := 100 - integer(o * DL)
         hls := h || l || ":100"
         c := HLSValue(hls)
         c := PaletteColor(p, PaletteKey(\p, c))
             
           Fg(c)
            DrawPoint(i,q,o)

  }
 }
} 
  
end

