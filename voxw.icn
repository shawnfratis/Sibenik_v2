procedure iv()
   local name, posx, posy

   #1st a dummy?

   args := ["d.png","1.png","2.png","3.png","4.png"]

   posx := posy := 20

   every name := !args do {
      WOpen("image=" || name, "posx=" || posx, "posy=" || posy) | {
         write(&errout, "*** cannot open image: ", name)
         next
         }
      repeat {
         case Event() of {
            "n":  {
               posx := WAttrib("posx")
               posy := WAttrib("posy")
               WClose()
               break
               }
            "q":  exit()#?????
            }
         }
      }

end
            
