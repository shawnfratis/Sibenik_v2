procedure comlist033(at)

(ax := at[1],bx := at[2],cx := at[3])
(dx := at[4],ex := at[5],fx := at[6])
(gx := at[7],hx := at[8],ix := at[9])
(jq := at[10])

c := colorTable(jq)

every x := 1 to (ax||bx||cx) do{ 
y := (dx||ex||fx) 
z := (gx||hx||ix) 

inpglcube01(x,y,z,1,c)

}

return()

end

